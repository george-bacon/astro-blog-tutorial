{
  "name": "planetary-pulsar",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "astro": "^4.3.5",
    "@astrojs/check": "^0.5.2",
    "typescript": "^5.3.3"
  },  
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0"
  },
  "prettier": {
    "astroAllowShorthand": true,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "pluginSearchDirs": [
      "."
    ],
    "plugins": [
      "prettier-plugin-astro"
    ]
  }
}